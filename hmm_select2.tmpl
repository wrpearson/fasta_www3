<!-- /TMPL_IF -->
</tr>
<tr>
<td colspan=2>
<b><font color="#990000">(A.2)</font> Or paste in Hidden Markov Model:</b><br>
<textarea name="hmm_query" rows=10 cols=80 wrap="off" align=left><!-- TMPL_VAR NAME=HMM_QUERY --></textarea>
</td>
</tr>
<tr><td colspan=2><hr></td></tr>
<!-- display the library table -->
