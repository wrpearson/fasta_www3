<tr>
<td>
<table>
<tr>
<th align="left">Protein</th>
<th align="left">DNA</th>
</tr>
<tr>
<td width="50%">
<select name="p_lib">
<!-- TMPL_LOOP NAME=LIB_P --> <option value="<!-- TMPL_VAR NAME=LIB_VAL -->" <!-- TMPL_VAR NAME=LIB_SELECT -->><!-- TMPL_VAR NAME=LIB_DESC --></option>
<!-- /TMPL_LOOP --></select>
</td>
<td width="50%">
<select name="n_lib">
<!-- TMPL_LOOP NAME=LIB_N --> <option value="<!-- TMPL_VAR NAME=LIB_VAL -->" <!-- TMPL_VAR NAME=LIB_SELECT -->><!-- TMPL_VAR NAME=LIB_DESC --></option>
<!-- /TMPL_LOOP --></select>
