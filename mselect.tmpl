<td align="center">
<!-- TMPL_IF NAME=DEBUG -->
<input type="checkbox" name="DEBUG" value="1" />debug
<!-- /TMPL_IF -->
<!-- TMPL_IF NAME=SHOW_REMOTE -->
<input type="checkbox" name="remote" value="1" />remote
<!-- /TMPL_IF -->
